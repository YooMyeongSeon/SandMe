DROP TABLE SANDME_MEMBERTBL;
DROP SEQUENCE SANDME_MEMBERSEQ;

CREATE TABLE SANDME_MEMBERTBL(
    memberNum NUMBER PRIMARY KEY,
    memberEmail VARCHAR2(50) NOT NULL UNIQUE,
    memberPwd VARCHAR2(200) NOT NULL,
    memberTel VARCHAR2(50) NOT NULL,
    memberName VARCHAR2(50) NOT NULL,
    admin varchar2(2)    
);

CREATE SEQUENCE SANDME_MEMBERSEQ;

SELECT * FROM SANDME_MEMBERTBL;

UPDATE SANDME_MEMBERTBL SET ADMIN = 'Y' WHERE memberemail='sandme@sandme.com';

COMMIT;